[queue:<%= @name %>]
<% unless @homogeneity.nil? -%>
homogeneity = <%= @homogeneity %>
<%end -%>
<% unless @comment.nil? -%>
comment = <%= @comment %>
<%end -%>
<% unless @condor_requirements.nil? -%>
condor_requirements = <%= @condor_requirements %>
<%end -%>
<% unless @totalcpus.nil? -%>
totalcpus = <%= @totalcpus %>
<%end -%>
<% unless @nodecpu.nil? -%>
nodecpu = <%= @nodecpu %>
<%end -%>
<% unless @nodememory.nil? -%>
nodememory = <%= @nodememory %>
<%end -%>
<% unless @defaultmemory.nil? -%>
defaultmemory = <%= @defaultmemory %>
<%end -%>
<% unless @architecture.nil? -%>
architecture = <%= @architecture %>
<%end -%>
<% @opsys.each do |os| -%>
opsys = <%= os %>
<% end -%>
<% unless @osname.nil? -%>
osname = <%= @osname %>
<%end -%>
<% unless @osversion.nil? -%>
osversion = <%= @osversion %>
<%end -%>
<% unless @osfamily.nil? -%>
osfamily = <%= @osfamily %>
<%end -%>
benchmark = <%= @primary_benchmark %> <%= @benchmark[@primary_benchmark] %>
<% @benchmark.keys.sort.each do |bm|
  if bm == @primary_benchmark then next end -%>
benchmark = <%= bm %> <%= @benchmark[bm] %>
<% end -%>
<% @access.each do |a|
  if a['groups'].is_a?(Array)
    a['groups'].each do |g| -%>
<%= a['mode']%>access = <%= g %>
<%  end
  else -%>
<%= a['mode']%>access = <%= a['groups'] %>
<% end
end -%>
<% @denyaccess.each do |da| -%>
denyaccess = <%= da %>
<% end -%>
<% @allowaccess.each do |aa| -%>
allowaccess = <%= aa %>
<% end -%>
<% @advertisedvo.each do |av| -%>
advertisedvo = <%= av %>
<% end -%>
<% unless @maxslotperjob.nil? -%>
maxslotperjob = <%= @maxslotperjob %>
<%end -%>
<% unless @forcedefaultvoms.nil? -%>
forcedefaultvoms = <%= @forcedefaultvoms %>
<%end -%>
<% unless @maxcputime.nil? -%>
maxcputime = <%= @maxcputime %>
<%end -%>
<% unless @mincputime.nil? -%>
mincputime = <%= @mincputime %>
<%end -%>
<% unless @maxwalltime.nil? -%>
maxwalltime = <%= @maxwalltime %>
<%end -%>
<% unless @minwalltime.nil? -%>
minwalltime = <%= @minwalltime %>
<%end -%>

