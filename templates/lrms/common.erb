[lrms]
# common lrms options
<% if @defaultqueue.nil? -%>
lrms = <%= @lrms %>
<% else -%>
lrms = <%= @lrms %> <%= @defaultqueue %>
<% end -%>
<% unless @lrmsconfig.nil? -%>
lrmsconfig = <%= @lrmsconfig %>
<%end -%>
<% @benchmark.keys.sort.each do |bm| -%>
benchmark = <%= bm %>:<%= @benchmark[bm] %>
<% end -%>
<% unless @defaultmemory.nil? -%>
defaultmemory = <%= @defaultmemory %>
<%end -%>
<% unless @nodename.nil? -%>
nodename = <%= @nodename %>
<%end -%>
<% unless @gnu_time.nil? -%>
gnu_time = <%= @gnu_time %>
<%end -%>
