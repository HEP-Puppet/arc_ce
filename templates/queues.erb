<% @queues.each_pair do |queue_name, queue_data| %>
[queue/<%= queue_name -%>]
name="<%= queue_name -%>"
<% if queue_data['default_memory'] -%>
defaultmemory=<%= queue_data['default_memory'] %>
<% end -%>
<% if queue_data['comment'] -%>
comment="<%= queue_data['comment'] -%>"
<% end -%>
<% if queue_data['homogeneity'] -%>
homogeneity="<%= queue_data['homogeneity'] -%>"
<% end -%>
<% if queue_data['main_memory_size'] -%>
MainMemorySize="<%= queue_data['main_memory_size'] -%>"
<% end -%>
<% if queue_data['time_limit'] -%>
timelimit=<%= queue_data['time_limit'] %>
<% end -%>
<% if queue_data['condor_requirements'] -%>
condor_requirements="<%= queue_data['condor_requirements'] -%>"
<% end -%>
<% if queue_data['cpudistribution'] -%>
cpudistribution=<%= queue_data['cpudistribution'].join(',') %>
<% end -%>
<% if queue_data['totalcpus'] -%>
totalcpus="<%= queue_data['totalcpus'] %>"
<% end -%>
<% if queue_data['authorized_vos'] -%>
<% queue_data['authorized_vos'].each do |val| -%>
authorizedvo="<%= val %>"
<% if queue_data['ac_policy'] == true -%>
ac_policy="VOMS: <%= val %>"
<% end -%>
<% end -%>
<% end -%>
<% end -%>
