[infosys/cluster]
<% unless @alias.nil? -%>
alias = <%= @alias %>
<% end -%>
hostname = <%= @hostname %>
<% unless @interactive_contactstring.nil? -%>
interactive_contactstring = <%= @interactive_contactstring %>
<% end -%>
<% unless @comment.nil? -%>
comment = <%= @comment %>
<% end -%>
<% unless @cluster_location.nil? -%>
cluster_location = <%= @cluster_location %>
<% end -%>
<% @cluster_owner.each do |co| -%>
cluster_owner = <%= co %>
<% end -%>
<% @advertisedvo.each do |co| -%>
advertisedvo = <%= co %>
<% end -%>
<% @clustersupport.each do |co| -%>
clustersupport = <%= co %>
<% end -%>
homogeneity = <%= @homogeneity %>
architecture = <%= @architecture %>
<% @opsys.each do |co| -%>
opsys = <%= co %>
<% end -%>
nodecpu = <%= @nodecpu %>
<% unless @nodememory.nil? -%>
nodememory = <%= @nodememory %>
<% end -%>
<% @middleware.each do |co| -%>
middleware = <%= co %>
<% end -%>
<% @nodeaccess.each do |co| -%>
nodeaccess = <%= co %>
<% end -%>
<% @localse.each do |co| -%>
localse = <%= co %>
<% end -%>
<% if @cpudistribution.size > 0 -%>
cpudistribution = <%= @cpudistribution.join(',') %>
<% end -%>
<% unless @maxcputime.nil? -%>
maxcputime = <%= @maxcputime %>
<% end -%>
<% unless @mincputime.nil? -%>
mincputime = <%= @mincputime %>
<% end -%>
<% unless @maxwalltime.nil? -%>
maxwalltime = <%= @maxwalltime %>
<% end -%>
<% unless @minwalltime.nil? -%>
minwalltime = <%= @minwalltime %>
<% end -%>

