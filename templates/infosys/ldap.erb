[infosys/ldap]
hostname = <%= @hostname %>
<% unless @slapd_hostnamebind.nil? -%>
slapd_hostnamebind = <%= @slapd_hostnamebind %>
<% end -%>
port = <%= @port %>
<% unless @user.nil? -%>
user = <%= user %>
<% end -%>
<% unless @slapd.nil? %>
slapd = <%= @slapd %>
<% end -%>
slapd_loglevel = <%= @slapd_loglevel %>
threads = <%= @threads %>
timelimit = <%= @timelimit %>
idletimeout = <%= @idletimeout %>
infosys_ldap_run_dir = <%= @infosys_ldap_run_dir %>
<% unless @ldap_schema_dir.nil? %>
ldap_schema_dir = <%= @ldap_schema_dir %>
<% end -%>
